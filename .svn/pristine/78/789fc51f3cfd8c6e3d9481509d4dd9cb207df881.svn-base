package com.cwkj.ysms.model.view;

/**
 * 运动员报名Excel导出视图类
 * @author zhangjiyao
 * @since 2015-3-13
 *
 */
public class ExcelAthleteView {
	
	private int athleteTeammemberId;
	public int getAthleteTeammemberId() {
		return athleteTeammemberId;
	}

	public void setAthleteTeammemberId(int athleteTeammemberId) {
		this.athleteTeammemberId = athleteTeammemberId;
	}
	private int athletePosition;//位置
	public int getAthletePosition() {
		return athletePosition;
	}

	public void setAthletePosition(int athletePosition) {
		this.athletePosition = athletePosition;
	}
	private Integer athleteNum;	//球衣号
	private String identifiedName;	//姓名
	private String identifiedId;	//身份证号码
	private String studyNum;	//学籍号
	private String athleteMobile;	//联系号码
	private Integer athleteHeight; //身高
	private Integer athleteWeight; //体重
	private String remark;	//备注
	
	public ExcelAthleteView() {
	}

	public ExcelAthleteView(Integer athleteNum, String identifiedName,
			String identifiedId, String studyNum, String athleteMobile,
			Integer athleteHeight, Integer athleteWeight, String remark) {
		this.athleteNum = athleteNum;
		this.identifiedName = identifiedName;
		this.identifiedId = identifiedId;
		this.studyNum = studyNum;
		this.athleteMobile = athleteMobile;
		this.athleteHeight = athleteHeight;
		this.athleteWeight = athleteWeight;
		this.remark = remark;
	}

	public Integer getAthleteHeight() {
		return athleteHeight;
	}

	public void setAthleteHeight(Integer athleteHeight) {
		this.athleteHeight = athleteHeight;
	}

	public Integer getAthleteWeight() {
		return athleteWeight;
	}

	public void setAthleteWeight(Integer athleteWeight) {
		this.athleteWeight = athleteWeight;
	}

	public Integer getAthleteNum() {
		return athleteNum;
	}

	public void setAthleteNum(Integer athleteNum) {
		this.athleteNum = athleteNum;
	}

	public String getIdentifiedName() {
		return identifiedName;
	}

	public void setIdentifiedName(String identifiedName) {
		this.identifiedName = identifiedName;
	}

	public String getIdentifiedId() {
		return identifiedId;
	}

	public void setIdentifiedId(String identifiedId) {
		this.identifiedId = identifiedId;
	}

	public String getStudyNum() {
		return studyNum;
	}

	public void setStudyNum(String studyNum) {
		this.studyNum = studyNum;
	}

	public String getAthleteMobile() {
		return athleteMobile;
	}

	public void setAthleteMobile(String athleteMobile) {
		this.athleteMobile = athleteMobile;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

}
