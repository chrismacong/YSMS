@CHARSET "UTF-8";

html {
	overflow-x: hidden;
}

.div_choose_leaguesystem {
	width: 700px;
	height: 530px;
	margin: 30px 0px 0px 150px;
}

.table_choose_leaguesystem {
	width: 100%;
	height: 100%;
	text-align: center;
}

.input_choose_leaguesystem {
	width: 200px;
	height: 200px;
	font-size: 1.0em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
}

.div_main {
	width: 1000px;
	height: 540px;
	margin: 5px auto;
}

.div_coachconfig_content {
	width: 1000px;
	height: auto;
	margin: 0px;
	padding: 0px;
	border: 1px solid #DDDDDD;
}

.div_configlist {
	width: 100%;
	line-height: 40px;
	height: 40px;
	cursor: pointer;
	background: #160b2d;
	vertical-align: center;
	color: #fff;
}

.img_configlist_titleico {
	height: 24px;
	margin: 8px 8px 8px 12px;
}

.div_configlist_content {
	width: 100%;
	height: 480px;
	background: #CFCFCF;
}

.div_configlist_content_left {
	float: left;
	width: 44%;
	height: 400px;
}

.operation {
	height: 140px;
	width: 1080px;
	background: #fff;
	color: #4f4f4f;
}

.operation ul {
	height: 100%;
	text-align: center;
	margin: 0 auto;
}

.operation .a_zone {
	width: 920px;
	margin: 0 auto;
}

.operation .a_zone li {
	float: left;
	width: 100px;
	height: 100%;
	margin: 0 60px;
	cursor: pointer;
}

.operation li div {
	width: 70px;
	margin: 15px;
}

.border_bo {
	border-bottom: 3px solid #f24a56
}

.div_configlist_content_left_top {
	width: 100%;
	height: 50px;
	line-height: 50px;
	vertical-align: middle;
}

.select_searchtype {
	width: 146px;
	margin: 0px 0px 2px 10px;
	padding: 0px 0px 0px 5px;
	height: 30px;
	line-height: 30px;
	vertical-align: middle;
	font-size: 0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

.input_text_search {
	margin: 12px 5px 12px 5px;
	width: 165px;
	height: 26px;
	padding: 0px 0px 0px 5px;
	font-size: 0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

.input_button_search {
	margin: 10px 0px 10px 0px;
	width: 80px;
	height: 30px;
	font-size: 0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

.table_configlist_title {
	width: 420px;
	height: 30px;
	margin: 0px 10px 0px 10px;
	color: #fff;
}

.table_configlist_title td {
	background: #160b2d;
	padding: 0px;
	margin: 0px;
	font-size: 1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	overflow: hidden;
	white-space: nowrap;
}

.div_configlist_contentlist {
	width: 420px;
	max-height: 390px;
	margin: 0px 10px 10px 10px;
	color: #fff;
	overflow-y: auto;
	overflow-x: hidden;
}

.div_configlist_contentlist td {
	background: #BBBBBB;
	padding: 0px;
	margin: 0px;
	font-size: 0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color: #000;
	border-bottom: 1px solid #fff;
	height: 30px;
	line-height: 30px;
	overflow: hidden;
	white-space: nowrap;
}

.td_firsttd_content {
	background: #BBBBBB;
	padding: 0px;
	margin: 0px;
	font-size: 0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color: #000;
	border-bottom: 1px solid #fff;
	border-right: 1px solid #fff;
	height: 30px;
	line-height: 30px;
}

.checkbox_style {
	width: 40px;
	height: 20px;
	line-height: 20px;
}

.input_button_addremove {
	width: 80px;
	height: 80px;
	font-size: 1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
}

.div_configlist_content_center {
	float: left;
	width: 12%;
	height: 400px;
}

.p_chooselist_title {
	height: 50px;
	line-height: 50px;
	color: #000;
	font-size: 1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	margin: 0px 0px 0px 10px;
}

.table_configchooselist_title {
	width: 400px;
	height: 30px;
	margin: 0px 10px 0px 10px;
	color: #fff;
}

.table_configchooselist_title td {
	background: #f24a56;
	padding: 0px;
	margin: 0px;
	font-size: 1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	overflow: hidden;
	white-space: nowrap;
}

.div_configchooselist_contentlist {
	width: 400px;
	max-height: 380px;
	margin: 0px 10px 10px 10px;
	color: #fff;
	overflow-y: auto;
	overflow-x: hidden;
}

.div_configchooselist_contentlist td {
	background: #BBBBBB;
	padding: 0px;
	margin: 0px;
	font-size: 0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color: #000;
	border-bottom: 1px solid #fff;
	height: 30px;
	line-height: 30px;
	overflow: hidden;
	white-space: nowrap;
}

.td_firstchoostd_content {
	background: #BBBBBB;
	padding: 0px;
	margin: 0px;
	font-size: 0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color: #000;
	border-bottom: 1px solid #fff;
	border-right: 1px solid #fff;
	height: 30px;
	line-height: 30px;
}

.div_coachconfig_content {
	width: 1000px;
	height: auto;
	margin-top: 5px;
	padding: 0px;
	border: 1px solid #DDDDDD;
}

.p_bottom {
	height: 60px;
	line-height: 60px;
	vertical-align: middle;
	text-align: right;
}

.input_button_okreset {
	width: 100px;
	height: 40px;
	font-size: 1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

#zone_frame {
	padding-top: 20px;
	border: none;
}

/*查看列表*/
#team_list, .list_info {
	margin-left: 2%;
	width: 98%;
	margin-top:20px;
	position: relative;
}

.table_head_left {
	position: absolute;
	width: 100px;
	height: 65px;
	background: #127419;
	top: -10px;
	left: -10px;
	line-height: 65px;
}

.table_head_left_fat {
	position: absolute;
	width: 150px;
	height: 65px;
	background: #f24a56;
	top: -10px;
	left: 0px;
	line-height: 65px;
}

#team_list thead, .list_info thead {
	background: #160b2d;
	color: #fff;
	font-size: 1.2em;
}

#team_list td, .list_info td {
	height: 47px;
	border-bottom: 1px solid #a7a7a7;
	text-align: center;
}

#team_list tbody td, .list_info tbody td {
	color: #160b2d;
}

.state {
	color: #f7715b !important;
}

.tbody_sroll {
	height: 448px;
	overflow: auto
}
#export_word{
	position:absolute;
	width:45px;
	height:45px;
	top:0px;
	background:url(../images/export_word.png) no-repeat center center;
	cursor:pointer;
}
#export_pdf{
	position:absolute;
	width:45px;
	height:45px;
	top:0px;
	background:url(../images/export_pdf.png) no-repeat center center;
	cursor:pointer;
}
