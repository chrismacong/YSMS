@CHARSET "UTF-8";
.div_main{
	width:1020px;
	height:570px;
	margin:5px auto;
}

.div_coachconfig_content{
	width:1000px;
	height:auto;
	margin:0px;
	padding:0px;
	border:1px solid #DDDDDD;
}

.div_configlist{
	width:100%;
	line-height:40px;
	height:40px;
	cursor: pointer;
	background: #160b2d;
	vertical-align: center;
	color:#fff;
}

.img_configlist_titleico{
	height:24px;
	margin:8px 8px 8px 12px;
}

.div_configlist_content{
	width:100%;
	height:360px;
	background:#CFCFCF;
}

.div_configlist_content_left{
	float:left;
	width:44%;
	height:360px;
}

.div_configlist_content_left_top{
	width:100%;
	height:50px;
	line-height:50px;
	vertical-align: middle;
}

.select_searchtype{
	width:146px;
	margin:0px 0px 2px 0px;
	height:30px;
	line-height:30px;
	vertical-align:middle;
	font-size:0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

.input_text_search{
	margin:12px 5px 2px 0px;
	width:135px;
	height:26px;
	font-size:0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

.input_button_search{
	margin:10px 0px 2px 0px;
	width:65px;
	height:30px;
	font-size:0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

.table_configlist_title{
	width:420px;
	height:30px;
	margin:0px 10px 0px 10px;
	color:#fff;
}

.table_configlist_title td{
	background: #160b2d;
	padding:0px;
	margin:0px;
	font-size:1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	overflow:hidden;
	white-space: nowrap;
}

.div_configlist_contentlist{
	width:420px;
	max-height:270px;
	margin:0px 10px 10px 10px;
	color:#fff;
	overflow-y:auto;
	overflow-x:hidden;
}

.div_configlist_contentlist td{
	background: #BBBBBB;
	padding:0px;
	margin:0px;
	font-size:0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color:#000;
	border-bottom: 1px solid #fff;
	height:30px;
	line-height:30px;
	overflow:hidden;
	white-space: nowrap;
}

.td_firsttd_content{
	background: #BBBBBB;
	padding:0px;
	margin:0px;
	font-size:0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color:#000;
	border-bottom: 1px solid #fff;
	border-right: 1px solid #fff;
	height:30px;
	line-height:30px;
}

.checkbox_style{
	width:40px;
	height:20px;
	line-height:20px;
}

.input_button_addremove{
	width:80px;
	height:80px;
	font-size:1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
}

.div_configlist_content_center{
	float:left;
	width:12%;
	height:360px;
}

.p_chooselist_title{
	height:50px;
	line-height:50px;
	color:#000;
	font-size:1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	margin:0px 0px 0px 10px;
}

.table_configchooselist_title{
	width:400px;
	height:40px;
	margin:0px 10px 0px 10px;
	color:#fff;
}

.table_configchooselist_title td{
	background: #f24a56;
	padding:0px;
	margin:0px;
	font-size:1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	overflow:hidden;
	white-space: nowrap;
}

.div_configchooselist_contentlist{
	width:400px;
	max-height:260px;
	margin:0px 10px 10px 10px;
	color:#fff;
	overflow-y:auto;
	overflow-x:hidden;
}

.div_configchooselist_contentlist td{
	background: #BBBBBB;
	padding:0px;
	margin:0px;
	font-size:0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color:#000;
	border-bottom: 1px solid #fff;
	height:40px;
	line-height:40px;
	overflow:hidden;
	white-space: nowrap;
}

.td_firstchoostd_content{
	background: #BBBBBB;
	padding:0px;
	margin:0px;
	font-size:0.9em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	text-align: center;
	color:#000;
	border-bottom: 1px solid #fff;
	border-right: 1px solid #fff;
	height:40px;
	line-height:40px;
}

.div_coachconfig_content{
	width:1000px;
	height:auto;
	margin-top:5px;
	padding:0px;
	border:1px solid #DDDDDD;
}

.p_bottom{
	height:60px;
	line-height:60px;
	vertical-align:middle;
	text-align: right;
}

.input_button_okreset{
	width:100px;
	height:40px;
	font-size:1em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
}

.input_leader_docter{
	margin-left:10px;
	height:28px;
	line-height:28px;
	width:200px;
	font-size:1.0em;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun';
	color: #251b3b;
	padding-left:10px;
}