*{margin:0;padding:0;}
	ul{ list-style: none}
		body {
		background:#4e7bb7;
		color:#fff;
		font-family: '寰蒋闆呴粦','榛戜綋';
		}
		.container{
			max-width: 640px;
			padding:0;
			margin:0 auto;
		}
		.bg{
			position:fixed;
			height:100%;
			width:100%;
			top:0;
			left:0;
			z-index:-1;
		}
		.bg img{
			height:100%;
			width:100%;
		}
		.bifen{
			width: 80%;
			height: 100px;
			margin: 0 auto;
			margin-top: 130px;
		}
		.bifen > div{
			float:left; 
			text-align: center;
			font-family: '寰蒋闆呴粦','榛戜綋';
			font-weight: bold;
		}
		.logo{
			height:80px;
			width:80px;
			margin:0 auto;
			border-radius:50px solid #fff;
			overflow: hidden; 
			background: url(../images/logo.png);
			background-size:100% 100%;
		}
		.logo_wk{
			height:100px;
			width:25%;
		}
		.logo_wk p{
			text-shadow: 2px 2px 5px #000;
		}
		.logo img{
			height:100%;
			width:auto;
		}
		.num{
			font-size: 50px;
			line-height: 100px;
			width:15%;
		}
		.name{
			width:100%;
			height: 190px;
			margin:0 auto;
			background: url(../images/name.png);
			background-size: 100% 100%;
			position: relative;
		}
		.name > div{
			position: absolute;
			width: 100%;
			text-align: center;
		}
		.vs{width:20%;}
		.vs img{
			margin-top:18px;
			
		}
		.matchlevel{
			height:30px;
			text-shadow:2px 2px 2px #333;
			font-size: 24px;
			top:15px;
			text-align: center;
		}
		.matchtime{
			  height: 26px;
			  text-shadow: 2px 2px 2px #333;
			  font-size: 18px;
			  bottom: 35px;	  
		}
		.leaguename{
			font-size: 42px;
			top: 50%;
			margin-top: -35px;
			font-weight: bold;
			letter-spacing:10px;
		}
		
		.wk_xijie{
			width:90%;
			min-height:200px;
			margin:0px auto 20px auto;

			background: url(../images/bg_op43.png);
			border-radius: 8px 8px 0 0;
			overflow: hidden;
		}
		.title{
			height:40px;
			color:#fff;	
		}
		.title_color1{
			background:#a09900;
		}
		.title_color2{
			background:#148182;
		}
		.title span{
		    float:left;
		    height: 24px;
		    width:24px;
		    margin:8px 20px 0 20px;
			background: #148182;
			text-align: center;
			border-radius: 12px
		}
		.title p{
			font-size:26px;
			
		}
		.jilu{
			width:90%;
			margin:10px auto;
			color:#000;
		}
		.title_2{
			border:5px;
			background: #160b2d;
			color:#fff;
			padding:5px;
			width:100px;
			text-align: center;
			border-radius: 5px
		}
		.jilu li{
			height:30px;
			line-height: 30px;
			margin:5px 12%;
		}
		.score span{
			display:block;
			float:left;
			height:30px;
			width:30px;
			background: url(../images/qiu.png);
			background-size: 100% 100%;
			margin-right:12%;
		}
		.foul span{
			display:block;
			float:left;
			height:30px;
			width:30px;
			background: url(../images/pai.png);
			background-size: 100% 100%;
			margin-right:12%;
		}
		.foul i{
			height:14px;
			width:11px;
			border-radius:3px;
			display: inline-block;
		} 
		.red_pai{
			background: red;
		}
		.yellow_pai{
			background:yellow;
		}
		.bottom{
			height:230px;
			background:url(../images/bg_bottom.png);
			background-size:100% 100%;
			position: relative;
		}
		.bottom div{
			text-align: center;
			width:243px;
			padding-top: 30%;
			margin:0 auto;
		}
		.bottom i{
			display: inline-block;
			height:21px;
			width:21px;
			float:left;
			background: url(../images/star.png);
			background-size:100% 100%; 
		}
		.bottom p{
			width:50%;
			min-width: 161px;
			margin:0 auto;
			float:left;
			padding:0 20px;
		}