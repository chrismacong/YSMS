	position:absolute;width:23px;height:23px;background:url(../images/grade_include_bg.png) no-repeat left center;color:#99CC33;font-size:20px;
}
.include_junior_grade{
	position:absolute;width:23px;height:23px;background:url(../images/grade_include_bg.png) no-repeat left center;color:#FF9900;font-size:20px;
}
.include_senior_grade{
	position:absolute;width:23px;height:23px;background:url(../images/grade_include_bg.png) no-repeat left center;color:#FFCC00;font-size:20px;
}
