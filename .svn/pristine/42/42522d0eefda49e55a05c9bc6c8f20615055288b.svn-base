body{
	overflow-x:hidden;
	overflow-y:hidden;
}
.input_wk {
	height: 30px;
	width: 350px;
	margin: 4px;
}

.input_l {
	float: left;
	height: 34px;
	width: 19px;
	background: url(../images/input_l_thin.png)
}

.input_m {
	float: left;
	height: 34px;
	width: 312px;
	background: url(../images/input_m_thin.png);
	position: relative;
	line-height: 30px
}

.input_r {
	float: left;
	height: 34px;
	width: 19px;
	background: url(../images/input_r_thin.png)
}

.input_text {
	text-align: left;
	line-height: 34px;
	height: 30px;
	margin-top: 2px;
	width: 100%;
	border: none;
	font-size: 1em;
	color: #251b3b;
	background: #fafafa;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun'
}

#button_read {
	border: none;
	width: 200px;
	height: 52px;
	cursor: pointer;
	background: url(../images/registerread.png) center center;
}

#button_reset {
	border: none;
	width: 200px;
	height: 52px;
	cursor: pointer;
	background: url(../images/registerreset.png) center center
}

#button_register {
	border: none;
	width: 200px;
	height: 52px;
	cursor: pointer;
	background: url(../images/register.png) center center
}

.center_table {
	margin-left: auto;
	margin-right: auto;
}

.operation {
	height: 140px;
	width: 1080px;
	background: #fff;
	color: #4f4f4f;
}

.operation ul {
	height: 100%;
	text-align: center;
	margin: 0 auto;
}

.operation .a_team {
	width: 960px;
	margin: 0 auto;
}

.operation .a_team_short {
	width: 520px;
	margin: 0 auto;
}

.operation .a_team li {
	float: left;
	width: 100px;
	height: 100%;
	margin: 0 60px;
	cursor: pointer;
}

.operation li div {
	width: 70px;
	margin: 15px;
}

.border_bo {
	border-bottom: 3px solid #127419
}

.neirong_wk {
	height: 515px;
	margin-top: 5px;
	background: #fff;
	padding-top: 0px;
	position: relative;
}

select {
	line-height: 30px;
	height: 30px;
	margin: -1px 0 0 -1px;
	width: 120%;
	border: none;
	font-size: 1em;
	color: #251b3b;
	background: #fafafa;
	font-family: 'Microsoft YaHei', 'SimHei', 'SimSun'
}

.select_icon {
	position: absolute;
	right: 2px;
	top: 8px;
	height: 12px;
	width: 16px;
	background: url(../images/select_icon.png);
	z-index: 2;
}

.select_wk {
	line-height: 30px;
	height: 30px;
	margin-top: 2px;
	width: 100%;
	border: none;
	overflow: hidden;
	position: relative;
	float: left;
	z-index: 1;
}

